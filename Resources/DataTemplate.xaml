<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CHMS.ViewModels"
                    xmlns:view="clr-namespace:CHMS.Views">


    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AvailableCarsVM}">
        <view:AvailableCarsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RentedCarsVM}">
        <view:RentedCarsView />
    </DataTemplate>

</ResourceDictionary>